<!DOCTYPE html>
<html>
<head>
  <title>Ratebeer</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="navibar">
    <%= link_to 'breweries', breweries_path %>
    <%= link_to 'beers', beers_path %>
    <%= link_to 'ratings', ratings_path %>
    <%= link_to 'users', users_path %>
    <%= link_to 'beer_clubs', beer_clubs_path %>
    <%= link_to 'places', places_path %>
    <%= link_to 'styles', styles_path %>
    |
    <% if(current_user == nil) %>
    	<%= link_to 'signIn', new_session_path, method: :get %>
    	<%= link_to 'signUp', new_user_path, method: :get %>
    <% else %>
    	<%= link_to current_user.username, user_path(current_user) %>
    	<%= link_to 'rate a beer', new_rating_path %>
    	<%= link_to 'signOut', '/signout', method: :delete %>
    	<%= link_to 'join to a club', new_membership_path %>
    <% end %>
</div>


<!--<% if not session[:user_id].nil? %>
  <p><%= User.find(session[:user_id]).username %> signed in</p>
<% end %>-->

<%= yield %>

</body>
</html>
